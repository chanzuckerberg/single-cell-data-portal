  "scripts": {
    "preinstall": "make retrieve-descendants",
    "dev": "next dev --experimental-https",
    "build": "NODE_OPTIONS=\"--max_old_space_size=2048\" next build",
    "start": "next start",
    "develop": "next dev --experimental-https",
    "format": "concurrently \"npx prettier --write .\" \"node_modules/.bin/next lint --fix\" \"node_modules/.bin/stylelint --fix '**/*.{js,ts,tsx,css}'\"",
    "serve": "next start",
    "test": "playwright test",
    "e2e-save": "playwright test",
    "e2e": "playwright test --project chromium",
    "e2e-pr": "playwright test --project $PROJECT --shard $SHARD --grep-invert @loggedIn",
    "e2e:chrome": "playwright test --project chromium",
    "e2e:chrome:headed": "playwright test --headed --project chromium",
    "e2e:ff": "playwright test --project firefox",
    "e2e:ie": "playwright test --project edge",
    "e2e-dev": "TEST_ENV=dev npm run e2e -- ",
    "e2e-dev-ci": "LOGIN=false TEST_ENV=dev playwright test --project $PROJECT --shard $SHARD --grep-invert @loggedIn",
    "e2e-dev-logged-in": "TEST_ENV=dev playwright test --project chromium --grep @loggedIn",
    "e2e-dev-save": "TEST_ENV=dev playwright test",
    "e2e-staging": "TEST_ENV=staging npm run e2e -- ",
    "e2e-staging-ci": "LOGIN=false TEST_ENV=staging playwright test --project $PROJECT --shard $SHARD --grep-invert @loggedIn",
    "e2e-staging-logged-in": "TEST_ENV=staging playwright test --project chromium --grep @loggedIn",
    "e2e-staging-save": "TEST_ENV=staging playwright test",
    "e2e-prod": "TEST_ENV=prod npm run e2e -- ",
    "e2e-prod-ci": "LOGIN=false TEST_ENV=prod playwright test --project $PROJECT --shard $SHARD --grep-invert @loggedIn",
    "e2e-prod-logged-in": "TEST_ENV=prod playwright test --project chromium --grep @loggedIn",
    "e2e-prod-save": "TEST_ENV=prod playwright test",
    "e2e-rdev": "DEPLOYMENT_STAGE=rdev TEST_ENV=rdev npm run e2e -- ",
    "e2e-rdev-ci": "LOGIN=false DEPLOYMENT_STAGE=rdev TEST_ENV=rdev playwright test --project $PROJECT --shard $SHARD --grep-invert @loggedIn",
    "e2e-rdev-logged-in": "DEPLOYMENT_STAGE=rdev TEST_ENV=rdev playwright test --project chromium --grep @loggedIn",
    "e2e-rdev-logged-in-ci": "DEPLOYMENT_STAGE=rdev TEST_ENV=rdev playwright test --project chromium --grep @loggedIn",
    "e2e-rdev-save": "TEST_ENV=rdev playwright test",
    "e2e-happy": "TEST_ENV=happy TEST_ACCOUNT_USER=User1 TEST_ACCOUNT_PASS=pwd npm run-script e2e",
    "type-check": "tsc --noEmit",
    "lint": "concurrently \"node_modules/.bin/next lint\" \"node_modules/.bin/stylelint '**/*.{js,ts,tsx,css}'\"  \"npm run type-check\"",
    "prettier-check": "npx prettier --check .",
    "prettier-fix": "npx prettier --write .",
    "build-and-start-prod": "npm run build && npm run serve",
    "postbuild": "next-sitemap",
    "e2e-trace": "npx playwright show-trace",
    "e2e-copy-snapshots-to-linux": "for file in __snapshots__/tests/features/**/*.test.ts-snapshots/*-darwin.txt; do cp \"$file\" \"${file/-darwin.txt/-linux.txt}\"; done"
  },
  "repository": {
    "type": "git",
